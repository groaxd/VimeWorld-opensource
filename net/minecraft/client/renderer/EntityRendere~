                     Lagometer.timerServer.end();
                     this.serverWaitTimeCurrent = this.serverWaitTime;
                  }

                  long long = System.nanoTime() / OBFVAL_0CDc9.get();
                  if(this.lastServerTime != 0L && this.lastServerTicks != 0) {
                     long long = long - this.lastServerTime;
                     if(long < 0L) {
